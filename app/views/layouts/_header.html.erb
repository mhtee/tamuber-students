<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
  <<%= link_to_if(logged_in?, "TamUber Students Interface", specify_path,
  id: "logo") {link_to("TamUber Students Interface", root_path, id: "logo")} %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if logged_in? %>

            <li><%= link_to "Home", specify_path %></li>
            <li><%= link_to "Log out", logout_path, method: :delete %></li>
    
        <% else %>
            <li><%= link_to "Home", root_path %></li>
            <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>